      "prefix": ["hle"],
      "body": "[$1]($2)$0",
      "description": "External link"
    },
    "h1": {
      "prefix": ["H1"],
      "body": "# $0",
      "description": "H1 link"
    },
    "h6": {
      "prefix": ["H6"],
      "body": "###### $0"
    },
    "fm": {
      "prefix": ["Fm"],
      "body": [
        "---",
        "title: $1",
        "date: \"$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\"",
        "tags: [\"$3\"]",
        "draft: true",
        "---",
        "",
        "$0"
      ],
      "description": "Front Matter"
    }
}
